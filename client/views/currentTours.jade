template(name="currentTours")
  .main-title-wrap
    h1.main-title Current Tours
  ul.tap-list.current-tours
    each tours
      li
        a(href="tour/#{_id}")
          +thumbnail tour=this
          h2.tap-list-title.tour-title #{mainTitle}
            span #{subTitle}
  +stopSearch state=buttonState stopNumbers=stopNumbers
